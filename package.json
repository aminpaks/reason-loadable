{
  "name": "reasonml-code-split",
  "version": "0.0.1",
  "scripts": {
    "dev:parcel": "cross-env NODE_ENV=development parcel index.html --no-cache",
    "build:parcel": "cross-env NODE_ENV=production parcel build index.html --public-url /",
    "dev:webpack": "cross-env NODE_ENV=development webpack-dev-server --mode development",
    "build:webpack": "cross-env NODE_ENV=production webpack --mode production",
    "clean": "bsb -clean-world",
    "make": "bsb -make-world",
    "test": "jest",
    "testw": "jest --watchAll",
    "up": "npm-update-all"
  },
  "devDependencies": {
    "autoprefixer": "8.0.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.3",
    "babel-preset-react-app": "3.1.1",
    "bs-jest": "0.3.2",
    "bs-platform": "2.2.1",
    "bs-react-test-renderer": "2.0.0",
    "bsb-js": "1.0.2",
    "cross-env": "5.1.3",
    "css-loader": "0.28.10",
    "html-webpack-plugin": "github:webpack-contrib/html-webpack-plugin",
    "jest": "22.4.2",
    "npm-update-all": "1.0.1",
    "parcel-bundler": "1.6.2",
    "style-loader": "0.20.2",
    "webpack": "4.0.1",
    "webpack-bundle-analyzer": "2.11.0",
    "webpack-cli": "2.0.9",
    "webpack-dev-server": "3.0.0"
  },
  "dependencies": {
    "history": "4.7.2",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "reason-react": "0.3.2",
    "universal-router": "6.0.0"
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "browsers": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 9"
        ],
        "flexbox": "no-2009"
      }
    }
  }
}
